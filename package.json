{
  "name": "fcl-kms-authorizer",
  "version": "0.0.13",
  "description": "AWS KMS authorizer (signer) for Flow blockchain.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json",
    "yarn.lock"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "test": "run-p test:*",
    "test:start-emulator": "docker-compose up",
    "test:execute": "sleep 10 && jest --forceExit && docker-compose down",
    "prepublishOnly": "yarn build",
    "example": "ts-node ./examples/send-tx.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doublejumptokyo/fcl-kms-authorizer.git"
  },
  "keywords": [
    "flow",
    "kms",
    "fcl",
    "onflow",
    "signer"
  ],
  "author": "double jump.tokyo Inc. <infra@doublejump.tokyo> (https://www.doublejump.tokyo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doublejumptokyo/fcl-kms-authorizer/issues"
  },
  "homepage": "https://github.com/doublejumptokyo/fcl-kms-authorizer#readme",
  "dependencies": {
    "@aws-sdk/client-kms": "^3.42.0",
    "@aws-sdk/credential-providers": "^3.42.0",
    "@onflow/fcl": "^0.0.77",
    "@onflow/rlp": "^0.0.3",
    "@onflow/types": "^0.0.5",
    "@types/asn1js": "^2.0.2",
    "@types/bn.js": "^5.1.0",
    "@types/keccak": "^3.0.1",
    "@types/node": "^16.11.10",
    "@types/secp256k1": "^4.0.3",
    "asn1js": "^2.1.1",
    "bn.js": "^5.2.0",
    "keccak": "^3.0.2",
    "rlp": "^2.2.7",
    "secp256k1": "^4.0.2",
    "sha3": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "elliptic": "^6.5.4",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "jest": {
    "testTimeout": 30000,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
